{
  "manifest_version": 2,

  "name": "Dev Buddy",
  "description": "Bookmark management tool",
  "version": "0.0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_action": {
    "default_icon": "logo192.png"
  },

  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["tabs"],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
